#include <bits/stdc++.h> 
#include <iostream>
using namespace std;

void bubbleSort(int *arr, int n)
{
    //Write your code here
    for(int i = 0 ; i<n ; i++){
        for(int j = 0 ; j<n-1-i ; j++){
            if(arr[j]>arr[j+1]){
                int temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }    
}

int main() {
    #ifndef ONLINE_JUDGE
        //for getting input from input.txt 
        freopen("inp.txt","r", stdin);
        // for writing output to output.txt
        freopen("out.txt","w", stdout);
    #endif

}